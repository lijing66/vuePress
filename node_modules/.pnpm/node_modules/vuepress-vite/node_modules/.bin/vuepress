#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/XiangMu/归档/github/vuePress/node_modules/.pnpm/vuepress-vite@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.3.8/node_modules/vuepress-vite/bin/node_modules:/mnt/d/XiangMu/归档/github/vuePress/node_modules/.pnpm/vuepress-vite@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.3.8/node_modules/vuepress-vite/node_modules:/mnt/d/XiangMu/归档/github/vuePress/node_modules/.pnpm/vuepress-vite@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.3.8/node_modules:/mnt/d/XiangMu/归档/github/vuePress/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/XiangMu/归档/github/vuePress/node_modules/.pnpm/vuepress-vite@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.3.8/node_modules/vuepress-vite/bin/node_modules:/mnt/d/XiangMu/归档/github/vuePress/node_modules/.pnpm/vuepress-vite@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.3.8/node_modules/vuepress-vite/node_modules:/mnt/d/XiangMu/归档/github/vuePress/node_modules/.pnpm/vuepress-vite@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.3.8/node_modules:/mnt/d/XiangMu/归档/github/vuePress/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress.js" "$@"
fi
